{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testazuredatafactory1719"
		},
		"LS_ADLSconnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSconnection'"
		},
		"LS_blobconnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blobconnection'"
		},
		"LS_sqlConnection_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_sqlConnection'"
		},
		"LS_ADLSconnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testdatalakegen21719.dfs.core.windows.net/"
		},
		"LS_BlobStorageConnect_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://testazureblobstorage1719.blob.core.windows.net/"
		},
		"LS_HTTPConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"LS_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testdatalakegen21719.dfs.core.windows.net/"
		},
		"LS_sqlConnection_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "testsequelserver1719.database.windows.net"
		},
		"LS_sqlConnection_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "testsequeldatabse1719"
		},
		"LS_sqlConnection_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "kalyan1719"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testazuredatafactory1719_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSconnection_accountKey": {
						"value": "[parameters('LS_ADLSconnection_accountKey')]"
					},
					"LS_blobconnection_connectionString": {
						"value": "[parameters('LS_blobconnection_connectionString')]"
					},
					"LS_sqlConnection_password": {
						"value": "[parameters('LS_sqlConnection_password')]"
					},
					"LS_ADLSconnection_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSconnection_properties_typeProperties_url')]"
					},
					"LS_HTTPConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTPConnection_properties_typeProperties_url')]"
					},
					"LS_sqlConnection_properties_typeProperties_server": {
						"value": "[parameters('LS_sqlConnection_properties_typeProperties_server')]"
					},
					"LS_sqlConnection_properties_typeProperties_database": {
						"value": "[parameters('LS_sqlConnection_properties_typeProperties_database')]"
					},
					"LS_sqlConnection_properties_typeProperties_userName": {
						"value": "[parameters('LS_sqlConnection_properties_typeProperties_userName')]"
					},
					"LS_BlobStorageConnect_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('LS_BlobStorageConnect_properties_typeProperties_serviceEndpoint')]"
					},
					"LS_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('LS_adlsgen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "testazuredatafactory1719_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testazuredatafactory1719_ArmTemplate_0')]"
			]
		},
		{
			"name": "testazuredatafactory1719_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testazuredatafactory1719_ArmTemplate_1')]"
			]
		}
	]
}